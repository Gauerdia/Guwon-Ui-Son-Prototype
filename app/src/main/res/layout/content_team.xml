        />
        <TextView
                android:text="@string/detlef_text"
                android:paddingRight="20dp"
                android:paddingLeft="0dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
