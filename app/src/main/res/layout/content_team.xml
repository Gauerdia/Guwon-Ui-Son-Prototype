<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="szymendera_development.guwon_app.TermineActivity"
    tools:showIn="@layout/app_bar_termine">

        <ImageView
        android:id="@+id/imageView_rolf"
        app:srcCompat="@drawable/rolf"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:paddingLeft="0dp"
        android:layout_marginTop="70dp"
        app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:text="@string/rolf_text"
            android:paddingRight="20dp"
            android:paddingLeft="0dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="70dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/imageView_rolf"
            android:ems="13"
            android:inputType="textMultiLine"
            />
        <ImageView
            android:id="@+id/imageView_mario"
            app:srcCompat="@drawable/mario"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:paddingLeft="0dp"
            app:layout_constraintTop_toBottomOf="@id/imageView_rolf"
            android:layout_marginTop="50dp"
/>
        <TextView
            android:text="@string/mario_text"
            android:paddingRight="20dp"
            android:paddingLeft="0dp"
            android:ems="13"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@id/imageView_rolf"
            app:layout_constraintLeft_toRightOf="@+id/imageView_mario"
            />
        <ImageView
                android:id="@+id/imageView_tobi"
                app:srcCompat="@drawable/tobi"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:paddingLeft="0dp"
                android:layout_marginTop="50dp"
                app:layout_constraintTop_toBottomOf="@id/imageView_mario"
        />
    <TextView
            android:text="@string/tobi_text"
            android:paddingRight="20dp"
            android:paddingLeft="0dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            app:layout_constraintLeft_toRightOf="@+id/imageView_tobi"
            app:layout_constraintTop_toBottomOf="@id/imageView_mario"
            android:ems="13"
            android:inputType="textMultiLine"
    />
        <ImageView
            android:id="@+id/imageView_maria"
            app:srcCompat="@drawable/maria"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:paddingLeft="0dp"
            android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@id/imageView_tobi"
/>
        <TextView
            android:text="@string/maria_text"
            android:paddingRight="20dp"
            android:paddingLeft="0dp"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            app:layout_constraintLeft_toRightOf="@+id/imageView_maria"
            app:layout_constraintTop_toBottomOf="@id/imageView_tobi"
            android:ems="13"
            android:inputType="textMultiLine"
            />
        <ImageView
                android:id="@+id/imageView_till"
                app:srcCompat="@drawable/till"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:paddingLeft="0dp"
                android:layout_marginTop="50dp"
                app:layout_constraintTop_toBottomOf="@id/imageView_maria"
        />
        <TextView
                android:text="@string/till_text"
                android:paddingRight="20dp"
                android:paddingLeft="0dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView_till"
                app:layout_constraintTop_toBottomOf="@id/imageView_maria"
                android:ems="13"
                android:inputType="textMultiLine"
        />
        <ImageView
                android:id="@+id/imageView_klaus"
                app:srcCompat="@drawable/klaus"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:paddingLeft="0dp"
                android:layout_marginTop="50dp"
                app:layout_constraintTop_toBottomOf="@id/imageView_till"
        />
        <TextView
                android:text="@string/klaus_text"
                android:paddingRight="20dp"
                android:paddingLeft="0dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView_klaus"
                app:layout_constraintTop_toBottomOf="@id/imageView_till"
                android:ems="13"
                android:inputType="textMultiLine"
        />
        <ImageView
                android:id="@+id/imageView_gisbert"
                app:srcCompat="@drawable/gisbert"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:paddingLeft="0dp"
                android:layout_marginTop="50dp"
                app:layout_constraintTop_toBottomOf="@id/imageView_klaus"
        />
        <TextView
                android:text="@string/gisbert_text"
                android:paddingRight="20dp"
                android:paddingLeft="0dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView_gisbert"
                app:layout_constraintTop_toBottomOf="@id/imageView_klaus"
                android:ems="13"
                android:inputType="textMultiLine"
        />
        <ImageView
                android:id="@+id/imageView_michael"
                app:srcCompat="@drawable/michael"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:paddingLeft="0dp"
                android:layout_marginTop="50dp"
                app:layout_constraintTop_toBottomOf="@id/imageView_gisbert"
        />
        <TextView
                android:text="@string/michael_text"
                android:paddingRight="20dp"
                android:paddingLeft="0dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView_michael"
                app:layout_constraintTop_toBottomOf="@id/imageView_gisbert"
                android:ems="13"
                android:inputType="textMultiLine"
        />
        <ImageView
                android:id="@+id/imageView_guenter"
                app:srcCompat="@drawable/guenter"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:paddingLeft="0dp"
                android:layout_marginTop="50dp"
                app:layout_constraintTop_toBottomOf="@id/imageView_michael"
        />
        <TextView
                android:text="@string/guenter_text"
                android:paddingRight="20dp"
                android:paddingLeft="0dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView_guenter"
                app:layout_constraintTop_toBottomOf="@id/imageView_michael"
                android:ems="13"
                android:inputType="textMultiLine"
        />

        <ImageView
                android:id="@+id/imageView_christiane"
                app:srcCompat="@drawable/christiane"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:paddingLeft="0dp"
                android:layout_marginTop="50dp"
                app:layout_constraintTop_toBottomOf="@id/imageView_guenter"
        />
        <TextView
                android:text="@string/christiane_text"
                android:paddingRight="20dp"
                android:paddingLeft="0dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView_christiane"
                app:layout_constraintTop_toBottomOf="@id/imageView_guenter"
                android:ems="13"
                android:inputType="textMultiLine"
        />
        <ImageView
                android:id="@+id/imageView_uwe"
                app:srcCompat="@drawable/uwe"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:paddingLeft="0dp"
                android:layout_marginTop="50dp"
                app:layout_constraintTop_toBottomOf="@id/imageView_christiane"
        />
        <TextView
                android:text="@string/uwe_text"
                android:paddingRight="20dp"
                android:paddingLeft="0dp"
                app:layout_constraintHorizontal_bias="0.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView_uwe"
                app:layout_constraintTop_toBottomOf="@id/imageView_christiane"
